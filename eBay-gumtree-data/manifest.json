{
    "manifest_version": 2,
    "name": "MCJ Gumtree eBay",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery-3.5.1.min.js","content.js"]
        }
      ],
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "img/icon.png"
    },
    "icons": { 
		"16": "img/16.png",				
		"32": "img/32.png",		
		"48": "img/48.png"
	  },
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [ 
        "contextMenus",
        "unlimitedStorage",
        "clipboardRead",
        "clipboardWrite",	
        "notifications"
    ]
}
